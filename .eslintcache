   \r\n        </div>\r\n    )\r\n    \r\n}\r\n\r\nconst mapStateToProps = (state) => {\r\n    return {\r\n        allItems: state.allItems\r\n        //add giftset - in giftse.js to all ITems\r\n    }\r\n}\r\n\r\nconst mapDispatchToProps = {\r\n    addToCart\r\n}\r\n\r\nexport default connect(mapStateToProps, mapDispatchToProps)(ItemDetail);","C:\\Coding\\Portfolio\\my-coffeeshop\\src\\components\\giftset-item\\giftset-item.js",[],"C:\\Coding\\Portfolio\\my-coffeeshop\\src\\components\\cart\\cart.js",[],"C:\\Coding\\Portfolio\\my-coffeeshop\\src\\components\\loading\\loading.js",[],"C:\\Coding\\Portfolio\\my-coffeeshop\\src\\components\\error\\error.js",[],"C:\\Coding\\Portfolio\\my-coffeeshop\\src\\components\\place-order\\place-order.js",[],"C:\\Coding\\Portfolio\\my-coffeeshop\\src\\components\\pages\\homePage.js",["134","135","136"],"import React from 'react';\r\nimport Beverages from '../beverages/beverages';\r\nimport Menu from '../menu/menu';\r\nimport Giftset from '../giftset/giftset';\r\nimport Combo from '../combo/combo';\r\nimport Items from '../items/items';\r\n\r\nconst HomePage = () => {\r\n    return (\r\n        <>\r\n            <Items type={'beverages'}/>\r\n            <Items type={'menuItems'}/>\r\n            <Giftset/>\r\n            <Items type={'combos'}/> \r\n        </>\r\n    )\r\n}\r\n\r\nexport default HomePage;","C:\\Coding\\Portfolio\\my-coffeeshop\\src\\components\\thank-you\\thank-you.js",[],"C:\\Coding\\Portfolio\\my-coffeeshop\\src\\services\\validate.js",[],"C:\\Coding\\Portfolio\\my-coffeeshop\\src\\assets\\basePath.js",[],"C:\\Coding\\Portfolio\\my-coffeeshop\\src\\redux\\actions\\cartAC.js",[],"C:\\Coding\\Portfolio\\my-coffeeshop\\src\\redux\\actions\\orderAC.js",[],"C:\\Coding\\Portfolio\\my-coffeeshop\\src\\redux\\actions\\giftset.js",[],"C:\\Coding\\Portfolio\\my-coffeeshop\\src\\redux\\store.js",[],"C:\\Coding\\Portfolio\\my-coffeeshop\\src\\redux\\reducer.js",[],"C:\\Coding\\Portfolio\\my-coffeeshop\\src\\firebase.config.js",[],"C:\\Coding\\Portfolio\\my-coffeeshop\\src\\components\\items\\items.js",["137"],"import React, {useState, useEffect, useMemo} from 'react';\r\n//import './combo.scss';\r\nimport Heading from '../heading/heading';\r\nimport ComboItem from '../combo-item/combo-item';\r\nimport MenuItem from '../menu-item/menu-item';\r\nimport BeverageItem from '../beverage-item/beverage-item';\r\nimport {connect} from 'react-redux';\r\nimport {addToCart, addToAllItems} from '../../redux/actions/cartAC';\r\nimport Loading from '../loading/loading';\r\nimport Error from '../error/error';\r\nimport firebase from '../../firebase.config';\r\n\r\nconst ComboTheme = React.lazy(() => import('./combo.scss'));\r\nconst MenuTheme = React.lazy(() => import('./menu.scss'));\r\nconst BeveragesTheme = React.lazy(() => import('./beverages.scss'));\r\n\r\nconst ThemeSelector = ({ children }, typeOfTheme) => {\r\n    //const CHOSEN_THEME = localStorage.getItem('TYPE_OF_THEME') || TYPE_OF_THEME.DEFAULT;\r\n    return (\r\n      <>\r\n        <React.Suspense fallback={<></>}>\r\n          {(typeOfTheme === 'combos') && <ComboTheme />}\r\n          {(typeOfTheme === 'menu') && <MenuTheme />}\r\n          {(typeOfTheme === 'beverages') && <BeveragesTheme />}\r\n        </React.Suspense>\r\n        {children}\r\n      </>\r\n    )\r\n}\r\n\r\nconst comboHeadingProps = {\r\n    id: 'combo',\r\n    small: 'Our artesan pastry',\r\n    big: 'ORGANIC INGREDIENTS ONLY'\r\n};\r\n\r\nconst menuHeadingProps = {\r\n    id: 'menu',\r\n    small: 'Choose Your Drink',\r\n    big: 'ORDER ONLINE AND SKIP THE LINE'\r\n};\r\n\r\nconst beveragesHeadingProps = {\r\n    id: 'beverages',\r\n    small: 'Your Personalized Coffee',\r\n    big: 'COFFEE BUILD YOUR BASE'\r\n};\r\n\r\nconst Items = (props) => {\r\n    console.log(props.allItems)\r\n    //console.log(props.type);\r\n    const [localState, setLocalState] = useState({\r\n        items: [],\r\n        loading: true,\r\n        error: false\r\n    });\r\n\r\n    const [isMoreBtnVisible, setMoreBtnVisible] = useState(true);\r\n\r\n    const showMore = () => {\r\n        setMoreBtnVisible(false);\r\n    }\r\n\r\n    const itemRef = useMemo( () => firebase.database().ref(props.type), [props.type]); // useMemo to add here\r\n\r\n    useEffect( () => {\r\n        itemRef.on('value', (snapshot) => {\r\n            const items = snapshot.val();\r\n            if (items) {\r\n                const itemList = [];\r\n                for (let id in items) {\r\n                    itemList.push({ id, ...items[id] });\r\n                };\r\n                props.addToAllItems(itemList);\r\n                setLocalState({\r\n                    items: props.type === 'menuItems' && isMoreBtnVisible ? itemList.slice(0,4) : itemList,\r\n                    loading: false,\r\n                    error: false\r\n                });\r\n                console.log(`${props.type} items received from server`);\r\n            }\r\n            else {\r\n                setLocalState((localState) => ({\r\n                    items: localState.itemList,\r\n                    loading: false,\r\n                    error: true\r\n                }));\r\n                console.log(`${props.type} - ERROR in fetching`);\r\n            }\r\n        });\r\n    }, [itemRef, isMoreBtnVisible])\r\n\r\n        if (localState.loading) {\r\n            return(\r\n                <Loading/>\r\n            )\r\n        }\r\n\r\n        else if (localState.error) {\r\n            return (\r\n                <Error/>\r\n            )\r\n        }\r\n\r\n        const headingProps = props.type === 'combos' ? comboHeadingProps : \r\n            props.type === 'menuItems' ? menuHeadingProps : beveragesHeadingProps;\r\n\r\n        return(\r\n            <ThemeSelector typeOfTheme={props.type}>\r\n            <section>\r\n                <Heading small={headingProps.small} big={headingProps.big} id={headingProps.id}/>\r\n                <div className={`${headingProps.id}_container`}>\r\n                    {\r\n                      props.type === 'combos' ? <div className=\"bg-combo\"></div> :\r\n                      props.type === 'menuItems' ? <div className=\"bg-menu\"></div> : null\r\n                    }\r\n                    \r\n                    {\r\n                        localState.items.map((item) => {\r\n                            if (props.type === 'menuItems') {\r\n                                return (\r\n                                    <MenuItem key={item.id} item={item} addToCart={() => props.addToCart(item)}/>\r\n                                )\r\n                            }\r\n                            else if (props.type === 'beverages') {\r\n                                return(\r\n                                    <BeverageItem key={item.id} item={item}/>\r\n                                )\r\n                            }\r\n                            else return(\r\n                                <ComboItem key={item.id} item={item} addToCart={() => props.addToCart(item)}/>\r\n                            )\r\n                        })\r\n                    }   \r\n                </div>\r\n                {\r\n                   props.type === 'menuItems' && isMoreBtnVisible ? <div className=\"menu_more\" onClick={showMore}>VIEW MORE</div> : null\r\n                }\r\n            </section>\r\n            </ThemeSelector>\r\n        )\r\n    \r\n}\r\n\r\nconst mapStateToProps = (state) => {\r\n    return {\r\n        allItems: state.allItems\r\n    }\r\n}\r\n\r\nconst mapDispatchToProps = {\r\n    /* combosLoaded,\r\n    combosRequested,\r\n    combosError, */\r\n    addToCart,\r\n    addToAllItems\r\n}\r\n\r\nexport default connect(mapStateToProps, mapDispatchToProps)(Items);","C:\\Coding\\Portfolio\\my-coffeeshop\\src\\components\\menu\\menu.js",[],"C:\\Coding\\Portfolio\\my-coffeeshop\\src\\components\\combo\\combo.js",[],"C:\\Coding\\Portfolio\\my-coffeeshop\\src\\compone